<?xml version="1.0" encoding="utf-8" ?>
<phpunit bootstrap="bootstrap.php"
		 strict="true"
		 checkForUnintentionallyCoveredCode="true">
	<testsuites>
		<testsuite name="RunFirst">
			<file>./RunFirst.php</file>
		</testsuite>
		<testsuite name="All">
			<directory>.</directory>
		</testsuite>
	</testsuites>
	<filter>
		<blacklist>
			<directory>..</directory>
		</blacklist>
		<whitelist>
			<directory suffix=".php">../src</directory>
		</whitelist>
	</filter>
</phpunit>